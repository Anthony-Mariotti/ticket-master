{
  "schemaVersion": 1,
  "id": "ticketmaster",
  "version": "${version}",
  "name": "Ticket Master",
  "description": "Fabric server ticketing system providing support to users in game.",
  "authors": [
    "Anthony Mariotti"
  ],
  "contact": {
    "homepage": "https://github.com/AMTraxTGE/ticket-master/wiki",
    "sources": "https://github.com/AMTraxTGE/ticket-master",
    "issues": "https://github.com/AMTraxTGE/ticket-master/issues"
  },
  "license": "GNU GPLv3",
  "icon": "assets/ticketmaster/icon.png",
  "environment": "*",
  "entrypoints": {
    "server": [
      {
        "adapter": "kotlin",
        "value": "com.anthonymariotti.ticketmaster.TicketMasterServer"
      }
    ]
  },
  "mixins": [
    "ticketmaster.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.18.2",
    "java": ">=17",
    "fabric-language-kotlin": ">=1.7.4+kotlin.1.6.21"
  },
  "suggests": {}
}
